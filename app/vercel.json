{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "framework": "create-react-app",
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/logo192.png",
            "dest": "/logo192.png"
        },
        {
            "src": "/logo512.png",
            "dest": "/logo512.png"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}